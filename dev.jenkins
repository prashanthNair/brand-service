pipeline {
  agent any
    
  tools {nodejs "node"}
    
  stages {
        
    stage('Git') {
      steps {
        git 'https://gitlab.com/migobucks_personal/migobucksbrandservice.git'
      }
    }
     
    stage('Build') {
      steps {
        sh 'npm install'
         sh 'npm run build'
      }
    }  
    
            
    stage('Start') {
      steps {
        sh 'npm run start'
      }
    }
  }
}